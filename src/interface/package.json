{
  "name": "voltaire-command-deck",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:grpc": "jest --testPathPattern=grpc",
    "profile": "next build --profile",
    "proto:generate": "protoc --plugin=protoc-gen-grpc-web=./node_modules/.bin/protoc-gen-grpc-web --plugin=protoc-gen-js=./node_modules/.bin/protoc-gen-js --js_out=import_style=commonjs,binary:./lib/grpc/generated --grpc-web_out=import_style=typescript,mode=grpcwebtext:./lib/grpc/generated --proto_path=../../protos ../../protos/reflex.proto",
    "clean": "rm -rf .next node_modules/.cache",
    "fresh": "npm run clean && npm run dev"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.14.3",
    "@grpc/proto-loader": "^0.8.0",
    "@react-three/drei": "9.122.0",
    "@react-three/fiber": "^8.16.8",
    "@use-gesture/react": "^10.3.1",
    "clsx": "2.1.1",
    "date-fns": "4.1.0",
    "framer-motion": "12.23.26",
    "google-protobuf": "4.0.1",
    "grpc-web": "^1.5.0",
    "lucide-react": "0.562.0",
    "next": "14.2.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "2.6.0",
    "three": "0.165.0",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.0",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/three": "^0.165.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.3",
    "jest": "^29.7.0",
    "postcss": "^8.4.38",
    "protoc-gen-grpc-web": "^1.5.0",
    "protoc-gen-js": "3.21.4-4",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "worker-loader": "3.0.8"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "overrides": {
    "react": "$react",
    "react-dom": "$react-dom"
  }
}
